        "bs58": "^5.0.0"
    },
    "devDependencies": {
        "@types/express": "^4.17.21",
        "@types/cors": "^2.8.17",
        "@types/node": "^20.10.5",
        "typescript": "^5.3.3",
        "ts-node": "^10.9.2",
        "ts-node-dev": "^2.0.0"
